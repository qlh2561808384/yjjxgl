.parser{ font-family: "microsoft yahei"; color: #5a5a5a;padding:0 20px;}
.parser .page-header h1 {  font-size: 30px;  font-weight: normal;}
.parser>pre{ -webkit-box-shadow: inset 30px 0 0 #fbfbfc, inset 31px 0 0 #ececf0;  -moz-box-shadow: inset 30px 0 0 #fbfbfc, inset 31px 0 0 #ececf0;
    box-shadow: inset 30px 0 0 #fbfbfc, inset 31px 0 0 #ececf0;margin:0 0 20px 0; padding:10px 0 30px 0; line-height:1px; }
.parser pre ol{ margin:0 0 -20px 35px; padding:0;}
.parser pre ol li{color:#bebec5;  line-height:25px;}
.parser pre ol li span{ color: #5a5a5a;}
.parser>table {width: 100%;margin-bottom: 20px;  }
.parser .table-border{border: 1px solid #dddddd;border-collapse: separate;border-left:0; -webkit-border-radius: 4px; -moz-border-radius: 4px;border-radius: 4px;}
.parser>table tbody tr:nth-child(odd) td {  background-color: #F5F5F5; }
.parser>table th, .parser>table td {  border-left: 1px solid #dddddd; border-top: 1px solid #dddddd;padding: 8px;  line-height: 20px;  text-align: left;  vertical-align: top;  }
.parser>table tr th {border-top:0;}