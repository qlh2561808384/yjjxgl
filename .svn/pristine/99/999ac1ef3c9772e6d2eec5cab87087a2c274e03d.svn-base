.parser{font-family:"microsoft yahei";color:#5a5a5a;padding:0 20px}.parser .page-header h1{font-size:30px;font-weight:normal}.parser>pre{-webkit-box-shadow:inset 30px 0 0 #fbfbfc,inset 31px 0 0 #ececf0;-moz-box-shadow:inset 30px 0 0 #fbfbfc,inset 31px 0 0 #ececf0;box-shadow:inset 30px 0 0 #fbfbfc,inset 31px 0 0 #ececf0;margin:0 0 20px 0;padding:10px 0 30px 0;line-height:1px}.parser pre ol{margin:0 0 -20px 35px;padding:0}.parser pre ol li{color:#bebec5;line-height:25px}.parser pre ol li span{color:#5a5a5a}.parser>table{width:100%;margin-bottom:20px}.parser .table-border{border:1px solid #ddd;border-collapse:separate;border-left:0;-webkit-border-radius:4px;-moz-border-radius:4px;border-radius:4px}.parser>table tbody tr:nth-child(odd) td{background-color:#f5f5f5}.parser>table th,.parser>table td{border-left:1px solid #ddd;border-top:1px solid #ddd;padding:5px;line-height:20px;text-align:left;vertical-align:top}.parser>table tr th{border-top:0}.siteDir{position:fixed;top:30%;right:10px;width:180px;height:340px;border-radius:3px;z-index:1000;border:1px solid #d2d2d2;background:#FFF}.siteDir .dir-title{height:40px;line-height:40px;padding:0 10px;font-size:14px;border-bottom:1px solid #eee;background-color:#f8f8f8}.siteDir .dir-title .close{line-height:40px;outline:0}.siteDir .dir-list{width:100%;height:300px;padding:10px 0;overflow:auto}.siteDir .dir-list .ztree{padding:0}.siteDir .dir-list li{padding-bottom:5px;font-size:14px}.outbackframe{padding:10px;background:#dedede;border:1px solid #DDD;border-radius:10px}.outbackframe .innerback{background:#fff;padding:10px;border-radius:10px}