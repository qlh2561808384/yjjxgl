@charset "UTF-8";

.idx_catagory{position: absolute; top:0; bottom:0; left:0; right:0; margin:auto; overflow: hidden;}
.idx_catagory li{float:left; width:100%; height:100%; text-align: center;}
.idx_catagory li a{display:block; width:100%; height:95%; margin:0 auto;}
.idx_catagory li a:active{padding:5px 0 0 5px;}

.catagoryNum1{width:28%;}
.catagoryNum1 li{width:100%;}

.catagoryNum3{width:85%;}
.catagoryNum3 li{width:33.3%;}

.catagoryNum2, .catagoryNum4{width:60%;}
.catagoryNum2 li, .catagoryNum4 li{width:50%;}
.catagoryNum4 li{height:50%;}

.catagoryNum1, .catagoryNum2, .catagoryNum3{height:30%;}
.catagoryNum4{height:60%;}